

#include <iostream>
using namespace std;

void toh(int n, int s, int d, int h)
{
	if(n == 1)
	{
		cout << "move plate " << n << " from " << s << " to " << d << endl;
		return;
	}

	toh(n - 1, s, h, d);
	cout << "move plate " << n << " from " << s << " to " << d << endl;
	toh(n - 1, h, d, s);
}


int main()
{
	int n;
	cin >> n;
	int s = 1, h = 2, d = 3;
	toh(n, s , d, h);
	return 0;
}
